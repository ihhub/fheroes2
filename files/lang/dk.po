msgstr ""

msgid "% chance to halve enemy"
msgstr ""

msgid "Soul Eater"
msgstr ""

msgid "Elemental"
msgstr "Elementalt"

msgid "No Morale"
msgstr "Ingen moral"

msgid "200% damage from Fire spells"
msgstr ""

msgid "200% damage from Cold spells"
msgstr ""


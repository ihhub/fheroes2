msgid "Blood Morale"
msgstr "Bra moral"

msgid "%{morale} Morale"
msgstr "%{morale} moral"

msgid "%{luck} Luck"
msgstr ""

msgid "Current Luck Modifiers:"
msgstr ""

msgid "Current Morale Modifiers:"

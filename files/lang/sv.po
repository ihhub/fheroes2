msgid "Wow, somebody has left a pile of wood here!"
msgstr "Oj, någon har lämnat en vedhög här!"

#: ../fheroes2/resource/maps_text.cpp:842
msgid "Wyrm"
msgstr "Wyrm"

#: ../fheroes2/resource/maps_text.cpp:843
msgid "Xabran"
msgstr "Xabran"

